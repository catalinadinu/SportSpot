        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAjiv8nBY9YEArqwST9vZM-tuJKYAFB5Yw" />

        <activity android:name=".TitlesReportActivity" />
        <activity android:name=".CoachesReportActivity" />
        <activity android:name=".TeamsReportActivity" />
        <activity android:name=".SportsReportsActivity" />
        <activity android:name=".VolleyballGamesActivity" />
        <activity android:name=".HandballGamesActivity" />
        <activity android:name=".FootballGamesActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".SportsListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>